{
  "name": "@my-org/vercel",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "vercel": "^27.2.0"
  },
  "devDependencies": {},
  "scripts": {
    "deploy-project": "npm run vercel-cli-pull && npm run vercel-cli-build && npm run vercel-cli-deploy",
    "vercel-cli-pull": "cd ../.. && vercel pull --debug --environment=production --token=$VERCEL_TOKEN",
    "vercel-cli-build": "cd ../.. && vercel build --debug --prod --token=$VERCEL_TOKEN",
    "vercel-cli-deploy": "cd ../.. && vercel deploy --debug --prebuilt --prod --token=$VERCEL_TOKEN"
  }
}
